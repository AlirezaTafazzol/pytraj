# distutils: language = c++

from libc.stdlib cimport malloc 

def  main_wrapper(cfunc, args):
     cdef char **c_argv
     args = [str(x) for x in args]
     c_argv = <char**>malloc(sizeof(char*) * len(args)) 
     for idx, s in enumerate(args):
         c_argv[idx] = s
     cfunc(len(args), c_argv)
